  },
  "peerDependencies": {
    "@floating-ui/dom": ">=1.0.0 <2.0.0"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@floating-ui/react": "^0.26.2",
    "@fluentui/react-components": "^9.41.0",
    "@fluentui/react-icons": "^2.0.217",
    "@fluentui/react-utilities": "^9.15.2",
    "@griffel/react": "^1.5",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@types/chrome": "^0.0.251",
    "@types/node": "^20.9.4",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/scheduler": "^0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.15",
    "react": "^17",
    "react-dom": "^17",
    "react-error-boundary": "^4.0.11",
    "react-json-view": "^1.21.3",
    "storybook": "^7.5.3",
    "typescript": "5.0.4",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3"
  },
  "packageManager": "yarn@4.0.2",
  "files": [
    "lib"
  ],
  "main": "./lib/floating-ui-devtools.cjs",
  "module": "./lib/floating-ui-devtools.js",
  "typings": "./lib/floating-ui-devtools.d.ts",
  "exports": {
    ".": {
      "types": "./lib/floating-ui-devtools.d.ts",
      "import": "./lib/floating-ui-devtools.js",
      "require": "./lib/floating-ui-devtools.umd.cjs"
    }
